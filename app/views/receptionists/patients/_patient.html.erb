<tr id="<%= dom_id(patient) %>">
          <td class="px-6 py-4"><%= patient.full_name %></td>
          <td class="px-6 py-4"><%= patient.date_of_birth %></td>
          <td class="px-6 py-4"><%= patient.phone %></td>
          <td class="px-6 py-4"><%= patient.email %></td>
          <td class="inline-flex pt-2">
            
            <%= link_to "Show", receptionists_patient_path(patient), data: { turbo: false }, class: "px-4 text-indigo-600 hover:text-indigo-900" %> | 
                  <%= link_to "Edit", edit_receptionists_patient_path(patient), data: { turbo: false }, class: "px-4 text-blue-600 hover:text-blue-700" %> |
                  <%= button_to "Delete", receptionists_patient_path(patient),
        method: :delete,
        form: { data: { turbo_confirm: "Are you sure you want to delete this patient?" } },
        class: "px-4 text-red-600 hover:text-red-700" %>
            </td>
        </tr>